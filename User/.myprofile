# source "/Users/archie/Documents/Me/Shell/User/.profile"

# Homebrew
eval "$(/opt/homebrew/bin/brew shellenv)"
export PATH="$PATH:/opt/homebrew/bin"

# Add RVM to PATH for scripting. Make sure this is the last PATH variable change.
export PATH="$PATH:$HOME/.rvm/bin"


# Java
export JAVA_HOME="/opt/homebrew/Cellar/openjdk/21.0.1/libexec/openjdk.jdk/Contents/Home"
export PATH="$JAVA_HOME/bin:$PATH"
export CPPFLAGS="-I$JAVA_HOME/include:$CPPFLAGS"

#adb
export PATH="$PATH:/Users/archie/Library/Android/sdk/platform-tools"

# Mysql
export PATH="$PATH:/usr/local/mysql/bin"

